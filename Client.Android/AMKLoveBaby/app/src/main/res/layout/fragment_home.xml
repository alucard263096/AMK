<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.helpfooter.steve.amklovebaby.HomeFragment"
    android:background="#F6F4F1">

    <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#F6F4F1"
        android:layout_marginBottom="10dp">
    <!-- TODO: Update blank fragment layout -->
        <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_heightPercent="40%"
            app:layout_marginBottomPercent="1%">
            <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_widthPercent="45%"
                android:background="#FFFFFF"
                android:orientation="vertical"
                android:id="@+id/doctor">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="30%"
                    app:layout_widthPercent="80%"
                    app:layout_marginLeftPercent="10%"
                    app:layout_marginTopPercent="20%"
                    android:src="@drawable/index_5" />
                <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="找医生"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:gravity="center"
                    android:clickable="false"
                    android:focusableInTouchMode="false"
                    android:id="@+id/findDoctor"/>
                <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="海量医生 为你服务"
                    android:textSize="12dp"
                    android:textColor="#919191"
                    android:gravity="top|center_horizontal"
                    android:clickable="false"
                    android:focusableInTouchMode="false"
                    android:id="@+id/findDoctorDesc"/>
            </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
            <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_widthPercent="55%"
                android:background="#F6F4F1"
                android:orientation="vertical"
                android:id="@+id/news">
                <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="100%"
                    app:layout_heightPercent="50%"
                    app:layout_marginLeftPercent="1%"
                    app:layout_marginRightPercent="1%"
                    android:background="#FFFFFF"
                    android:id="@+id/lvTaiwanDoctor"
                    android:tag="is_taiwan='Y'">
                    <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_widthPercent="40%"
                        app:layout_heightPercent="120%"
                        app:layout_marginLeftPercent="10%"
                        app:layout_marginTopPercent="0%"
                        android:background="#FFFFFF"
                        android:orientation="vertical">
                        <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="海外医生"
                            app:layout_heightPercent="50%"
                            android:textSize="15dp"
                            android:textColor="#000000"
                            android:gravity="bottom|left"
                            android:id="@+id/quickAsk" />
                        <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="一流国际服务"
                            app:layout_heightPercent="50%"
                            android:textSize="12dp"
                            android:textColor="#919191"
                            android:paddingTop="0dp"
                            android:gravity="top|left"
                            android:id="@+id/quickAskDesc" />
                    </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                    <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        app:layout_marginLeftPercent="0%"
                        app:layout_marginRightPercent="0%"
                        app:layout_marginTopPercent="0%"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:id="@+id/quickAskPicture">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_heightPercent="50%"
                            android:src="@drawable/index_13" />
                    </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="100%"
                    app:layout_heightPercent="50%"
                    app:layout_marginLeftPercent="1%"
                    app:layout_marginRightPercent="1%"
                    app:layout_marginTopPercent="1%"
                    android:orientation="horizontal">
                    <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        android:background="#FFFFFF"
                        android:layout_marginRight="2px"
                        android:orientation="vertical"
                        android:id="@+id/lvVideoDoctor"
                        android:tag="enable_videochat='Y' order by videoquerycount desc">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_heightPercent="45%"
                            app:layout_marginTopPercent="8%"
                            android:src="@drawable/index_14" />
                        <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="视频会诊"
                            android:textSize="15dp"
                            android:textColor="#000000"
                            android:gravity="center"
                            android:id="@+id/quickShiping" />
                    </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                    <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_widthPercent="50%"
                        android:background="#FFFFFF"
                        app:layout_heightPercent="100%"
                        android:orientation="vertical"
                        android:id="@+id/lvCharDoctor"
                        android:tag="enable_charchat='Y' order by charquerycount desc">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_heightPercent="45%"
                            app:layout_marginTopPercent="8%"
                            android:src="@drawable/index_6" />
                        <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="图文咨询"
                            android:textSize="15dp"
                            android:textColor="#000000"
                            android:gravity="center"
                            android:id="@+id/quickTuwen" />
                    </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>

            </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
        </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>

        <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_heightPercent="36%"
            app:layout_widthPercent="100%"
            app:layout_marginLeftPercent="0%"
            app:layout_marginRightPercent="0%"
            android:orientation="vertical"
            android:layout_margin="0dp"
            android:background="#ffffff">
            <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_heightPercent="30%"
                app:layout_widthPercent="100%"
                app:layout_marginLeftPercent="0%"
                app:layout_marginRightPercent="0%"
                android:layout_margin="0dp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    app:layout_marginLeftPercent="0%"
                    app:layout_marginTopPercent="0%"
                    android:layout_gravity="left|bottom"
                    android:src="@drawable/index_12"/>
            </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
            <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_heightPercent="70%"
                app:layout_widthPercent="100%"
                app:layout_marginLeftPercent="0%"
                app:layout_marginRightPercent="0%"
                android:layout_margin="0dp">
                <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="33%"
                    app:layout_marginLeftPercent="0%"
                    app:layout_marginRightPercent="0%"
                    app:layout_marginTopPercent="0%"
                    app:layout_marginBottomPercent="1%"
                    android:background="#FFFFFF"
                    android:orientation="vertical"
                    android:id="@+id/askForHealth">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="30%"
                        app:layout_marginTopPercent="10%"
                        android:src="@drawable/index_11"/>
                    <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textSize="14dp"
                        android:textColor="#919191"
                        app:layout_heightPercent="30%"
                        app:layout_marginLeftPercent="35%"
                        android:text="健康咨询"
                        android:gravity="bottom"
                        android:id="@+id/healthAsk"/>
                </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="33%"
                    app:layout_marginLeftPercent="0%"
                    app:layout_marginRightPercent="0%"
                    app:layout_marginTopPercent="1%"
                    app:layout_marginBottomPercent="1%"
                    android:background="#FFFFFF"
                    android:orientation="vertical"
                    android:id="@+id/pregnancy">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="25%"
                        app:layout_marginTopPercent="10%"
                        android:src="@drawable/index_9" />
                    <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textSize="14dp"
                        android:textColor="#919191"
                        app:layout_heightPercent="30%"
                        app:layout_marginRightPercent="15%"
                        android:gravity="bottom|right"
                        android:text="孕产常识"
                        android:id="@+id/newsList2"/>
                </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
                <com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    app:layout_widthPercent="33%"
                    app:layout_marginLeftPercent="0%"
                    app:layout_marginRightPercent="0%"
                    app:layout_marginTopPercent="1%"
                    app:layout_marginBottomPercent="1%"
                    android:background="#FFFFFF"
                    android:orientation="vertical"
                    android:id="@+id/earlierStudy">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        app:layout_heightPercent="50%"
                        app:layout_marginLeftPercent="20%"
                        app:layout_marginTopPercent="10%"
                        android:src="@drawable/index_10" />
                    <com.helpfooter.steve.amklovebaby.CustomControlView.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textSize="14dp"
                        android:textColor="#919191"
                        app:layout_heightPercent="30%"
                        app:layout_marginRightPercent="15%"
                        android:gravity="bottom|right"
                        android:text="早教方法"
                        android:id="@+id/newsList1"/>
                </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
            </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
        </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>
        <com.helpfooter.steve.amklovebaby.CustomControlView.IndexBannerSilderView
            android:id="@+id/imageSlider"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="1%"
            android:flipInterval="5000"
            app:layout_heightPercent="23%" >
        </com.helpfooter.steve.amklovebaby.CustomControlView.IndexBannerSilderView>
    </com.helpfooter.steve.amklovebaby.Extents.PercentLayout.PercentLinearLayout>


</FrameLayout>

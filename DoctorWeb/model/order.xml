<?xml version="1.0" encoding="utf-8" ?>
<root>
  <name>订单</name>
  <tablename>v_order_full</tablename>
  <searchcondition>r_main.status&lt;&gt;'D'</searchcondition>
  <fields>
    <field>
      <name>订单编号</name>
      <key>order_no</key>
      <type>text</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <notnull>1</notnull>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>预约日期</name>
      <key>order_date</key>
      <type>datetime</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>预约时间</name>
      <key>order_time</key>
      <type>text</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>价格</name>
      <key>price</key>
      <type>text</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>折扣</name>
      <key>discount</key>
      <type>text</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>状态</name>
      <key>status</key>
      <type>select</type>
      <displayinlist>1</displayinlist>
      <options>
        <option>
          <name>订单创建</name>
          <value>T</value>
        </option>
        <option>
          <name>等待处理</name>
          <value>P</value>
        </option>
        <option>
          <name>完成订单</name>
          <value>F</value>
        </option>
        <option>
          <name>取消</name>
          <value>C</value>
        </option>
      </options>
      <notnull>1</notnull>
      <search>1</search>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>备注描述</name>
      <key>remark</key>
      <type>longtext</type>
      <notnull>1</notnull>
    </field>
  </fields>
  <javascripts>
    <javascript>
      <filename>order.js</filename>
      <detail>1</detail>
    </javascript>
    <javascript>
    </javascript>
  </javascripts>
</root>
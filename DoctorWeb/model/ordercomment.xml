<?xml version="1.0" encoding="utf-8" ?>
<root>
  <name>客户订单评价回复</name>
  <tablename>v_order_full</tablename>
  <searchcondition>r_main.status='F' and r_main.hascomment='Y' and isnull(r_main.reply,'')='' </searchcondition>
  <fields>
    <field>
      <name>订单编号</name>
      <key>order_no</key>
      <type>text</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>评价时间</name>
      <key>comment_date</key>
      <type>datetime</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>服务质量评分</name>
      <key>service</key>
      <type>number</type>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>医生水平评分</name>
      <key>ability</key>
      <type>number</type>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>客户评价</name>
      <key>comment</key>
      <type>longtext</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>我的回复</name>
      <key>reply</key>
      <type>longtext</type>
      <notnull>1</notnull>
    </field>
    <field>
      <name>回复时间</name>
      <key>reply_date</key>
      <type>datetime</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
  </fields>
  <javascripts>
    <javascript>
      <filename>ordercomment.js</filename>
      <detail>1</detail>
    </javascript>
    <javascript>
    </javascript>
  </javascripts>
</root>